{
  "name": "lambda-express",
  "version": "1.0.0",
  "description": "Express server deployment using AWS Lambda.",
  "license": "",
  "author": "",
  "sideEffects": false,
  "main": "src/module.ts",
  "scripts": {
    "build": "ts-node --swc ./scripts/build.ts",
    "clean": "rimraf ./dist && rimraf distLambda",
    "compile": "tsc --build",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js --runInBand",
    "deploy": "yarn build && yarn template deploy \"$@\"",
    "deploy-ts": "yarn build && yarn template-ts deploy",
    "infra": "yarn template infra \"$@\"",
    "start": "yarn compile && yarn node dist/src/local.js",
    "template": "yarn template-ts",
    "template-ts": "ts-node --swc scripts/template.ts",
    "test": "yarn test-ci --watch",
    "test-ci": "TEST_SERVER_PORT=5030 CORS=http://localhost:3000 GOLDSTACK_DEPLOYMENT=local jest --passWithNoTests --config=jest.config.js --runInBand",
    "watch": "PORT=3030 CORS=http://localhost:3000 GOLDSTACK_DEPLOYMENT=local ts-node-dev --respawn ./src/local.ts"
  },
  "dependencies": {
    "@goldstack/template-lambda-express": "0.5.18",
    "@goldstack/utils-esbuild": "0.5.9",
    "aws-serverless-express": "^3.3.8",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "camelcase-keys": "^6.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "4.17.3",
    "express-mung": "^0.5.1",
    "express-validation": "^3.0.6",
    "helmet": "^4.6.0",
    "http-logger": "git+https://github.com/minhqp/http-logger.git",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.2",
    "mongoose": "^5.9.25",
    "morgan": "^1.10.0",
    "snakecase-keys": "^3.2.0",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.5",
    "@swc/core": "^1.3.74",
    "@swc/jest": "^0.2.27",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.13",
    "@types/source-map-support": "^0.5.4",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.15",
    "esbuild": "^0.17.18",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.0.1",
    "supertest": "^4.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
